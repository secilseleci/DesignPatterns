@model DesignPattern.Mediator.MediatorPattern.Results.UpdateProductByIDQueryResult 


<link href="~/lib/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet" />
<link href="~/lib/bootstrap/dist/css/bootstrap.rtl.min.css" rel="stylesheet" />
<link href="~/lib/bootstrap/dist/css/bootstrap.rtl.css" rel="stylesheet" />
<link href="~/lib/bootstrap/dist/css/bootstrap.css" rel="stylesheet" />

@{
    Layout = null;
}
<div class="container">
    <form method="post">
        <label>Ürün id</label>
        <input type="text" asp-for="ProductID" class="form-control" />
        <br />
        <label>Ürün Ad</label>
        <input type="text" asp-for="ProductName" class="form-control" />
        <br />
        <label>Ürün Fiyat</label>
        <input type="text" asp-for="ProductPrice" class="form-control" />
        <br />
        <label>Ürün Stok</label>
        <input type="text" asp-for="ProductStock" class="form-control" />
        <br />
        <label>Stok tipi</label>
        <input type="text" asp-for="ProductStockType" class="form-control" />
        <br />
        <br />
        <label>Ürün kategorisi</label>
        <input type="text" asp-for="ProductCategory" class="form-control" />
        <br />
        <button class="btn btn-info">Güncelle</button>
    </form>



</div>